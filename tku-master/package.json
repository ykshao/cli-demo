{
  "name": "tku",
  "version": "1.0.0",
  "description": "Lightweight Mobile UI Components built on Vue",
  "main": "lib/index.js",
  "module": "es/index.js",
  "style": "lib/tku-css/index.css",
  "typings": "types/index.d.ts",
  "files": [
    "es",
    "lib",
    "packages",
    "types"
  ],
  "scripts": {
    "bootstrap": "yarn || npm i && cd ./packages/tku-css/ && yarn || npm i && cd ../../",
    "dev": "npm run build:file && webpack-serve --config build/webpack.dev.js",
    "lint": "eslint ./packages --ext .js,.vue && stylelint \"packages/tku-css/src/*.css\"",
    "build:file": "node build/build-entry.js",
    "build:components": "node build/build-components.js --color",
    "build:tku-css": "gulp build --gulpfile packages/tku-css/gulpfile.js --color --silent",
    "build:style-entry": "node build/build-style-entry.js",
    "build:iconfont": "gulp --gulpfile ./build/build-iconfont.js",
    "build:site": "rimraf docs/dist && cross-env NODE_ENV=production webpack --config build/webpack.doc.js"
  },
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "6.x",
    "vue-lazyload": "^1.2.6"
  },
  "peerDependencies": {
    "vue": ">= 2.5.0"
  },
  "devDependencies": {
    "@vue/server-test-utils": "^1.0.0-beta.20",
    "@vue/test-utils": "^1.0.0-beta.20",
    "autoprefixer": "^8.6.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "codecov": "^3.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "dependency-tree": "^6.1.0",
    "eslint": "^5.0.1",
    "eslint-plugin-vue-libs": "^3.0.0",
    "fast-glob": "^2.2.2",
    "fast-vue-md-loader": "^1.0.3",
    "gulp": "^3.9.1",
    "gulp-iconfont": "^10.0.1",
    "gulp-iconfont-css": "^2.1.0",
    "html-webpack-plugin": "3.2.0",
    "lint-staged": "^7.2.0",
    "md5-file": "^4.0.0",
    "postcss": "^6.0.23",
    "postcss-calc": "^6.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.5",
    "precss": "2.0.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "rimraf": "^2.5.4",
    "shelljs": "^0.8.1",
    "signale": "^1.2.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.0.1",
    "vant-doc": "1.0.7",
    "vue": "2.5.16",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.2.4",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vue-sfc-compiler": "^0.0.8",
    "vue-template-compiler": "2.5.16",
    "vue-template-es2015-compiler": "^1.6.0",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-serve": "^1.0.4"
  }
}
